# Ultrathink Configuration
# Copy to .env and fill in your values

# Telegram Bot
# Create bot via @BotFather, get chat ID via @userinfobot
TELEGRAM_BOT_TOKEN=1234567890:ABCdefGHIjklMNOpqrsTUVwxyz
TELEGRAM_CHAT_ID=123456789

# Anthropic API (for Claude classification)
# https://console.anthropic.com/
ANTHROPIC_API_KEY=sk-ant-api03-xxxxxxxxxxxx

# OpenAI API (for Whisper voice transcription)
# https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-xxxxxxxxxxxx

# Optional settings
TIMEZONE=Australia/Brisbane
CONFIDENCE_THRESHOLD=0.6

# Container user (match host user for vault ownership)
PUID=1000
PGID=1000

# Web UI (required for web access)
WEB_USERNAME=admin
WEB_PASSWORD=change-me
WEB_SECRET=
